on boot
# Performance Profiles Power save
on property:sys.perf.profile=0
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq "936000"
    write /sys/devices/system/cpu/cpu1/cpufreq/scaling_max_freq "936000"
    write /sys/devices/system/cpu/cpu2/cpufreq/scaling_max_freq "936000"
    write /sys/devices/system/cpu/cpu3/cpufreq/scaling_max_freq "936000"
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq "468000"
    write /sys/devices/system/cpu/cpu1/cpufreq/scaling_min_freq "468000"
    write /sys/devices/system/cpu/cpu2/cpufreq/scaling_min_freq "468000"
    write /sys/devices/system/cpu/cpu3/cpufreq/scaling_min_freq "468000"
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor 
"powersave"
    write /sys/devices/system/cpu/cpu1/cpufreq/scaling_governor 
"powersave"
    write /sys/devices/system/cpu/cpu2/cpufreq/scaling_governor 
"powersave"
    write /sys/devices/system/cpu/cpu3/cpufreq/scaling_governor 
"powersave"
# Balanced (normal, default as stock)
on property:sys.perf.profile=1
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq 
"1794000"
    write /sys/devices/system/cpu/cpu1/cpufreq/scaling_max_freq 
"1794000"
    write /sys/devices/system/cpu/cpu2/cpufreq/scaling_max_freq 
"1794000"
    write /sys/devices/system/cpu/cpu3/cpufreq/scaling_max_freq 
"1794000"
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq "468000"
    write /sys/devices/system/cpu/cpu1/cpufreq/scaling_min_freq "468000"
    write /sys/devices/system/cpu/cpu2/cpufreq/scaling_min_freq "468000"
    write /sys/devices/system/cpu/cpu3/cpufreq/scaling_min_freq "468000"
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor 
"interactive"
    write /sys/devices/system/cpu/cpu1/cpufreq/scaling_governor 
"interactive"
    write /sys/devices/system/cpu/cpu2/cpufreq/scaling_governor 
"interactive"
    write /sys/devices/system/cpu/cpu3/cpufreq/scaling_governor 
"interactive"
    
